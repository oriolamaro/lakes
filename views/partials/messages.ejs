<% if(typeof errors!= 'undefined') { %>
<%    errors.forEach(function(error){ %>
<p class="error_msg"><%= error.msg %></p>
<%    })        %>
<% } %>

<% if(success_msg!= '') { %>
<p class="success_msg"><%=success_msg %> </p>
<% } %>

<% if(error_msg != '') { %>
<%= error_msg %> 
<% } %> 
<% if(error != '') { %>
<%= error %>
<% } %>
<br>